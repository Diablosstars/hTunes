<Window x:Class="hTunes.RenameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:hTunes"
        mc:Ignorable="d"
        Title="RenameWindow" Height="300" Width="300">
    <Grid>
        <TextBox Name ="textBox1" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="Enter Playlist Name..." VerticalAlignment="Top" Width="272"/>
        <Button Content="Rename" Name="Rename" HorizontalAlignment="Left" Margin="125,39,0,0" VerticalAlignment="Top" Click="Rename_Click" Width="75"/>
        <Button Content="Cancel" Name="Cancel" HorizontalAlignment="Left" Margin="207,39,0,0" VerticalAlignment="Top" Click="Cancel_Click" Width="75"/>

    </Grid>
</Window>
